<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Pieterjan De Clippel</Authors>
    <Company>MintPlayer</Company>
    <Description>This package allows you to retrieve the web browsers installed on the system.</Description>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/MintPlayer/PlatformBrowser</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MintPlayer/PlatformBrowser</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Webbrowser, Dialog</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="4.6.0" />
  </ItemGroup>

</Project>
